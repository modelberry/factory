{
  "name": "@modelberry/any",
  "version": "6.25.4",
  "description": "Any component with CSS element reset and CSS Emotion props",
  "keywords": [
    "any",
    "cli",
    "component",
    "contentful",
    "gatsbyjs",
    "netlify",
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/jaccomeijer/modelberry#readme",
  "bugs": {
    "url": "https://github.com/jaccomeijer/modelberry/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaccomeijer/modelberry.git"
  },
  "license": "MIT",
  "author": "Jacco Meijer (https://www.jaccomeijer.nl)",
  "contributors": [
    {
      "name": "Jacco Meijer",
      "email": "jacco@jaccomeijer.nl",
      "url": "https://www.jaccomeijer.nl"
    }
  ],
  "exports": {
    "./react": {
      "import": "./build/react.mjs",
      "require": "./build/react.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "build/*"
      ]
    }
  },
  "files": [
    "CHANGELOG.md",
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:compile": "rollup --config",
    "build:lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "build:test": "jest --silent --passWithNoTests",
    "build:types": "tsc -p tsconfig.json --declaration --emitDeclarationOnly --declarationDir build",
    "clean": "rimraf build/*",
    "dev": "rollup --config --watch",
    "generate": "npm-run-all generate:*",
    "generate:compile": "tsc -p tsconfig.generate.json",
    "generate:update": "node build/generate-elements/generate-elements.js",
    "generate:update-lint-fix": "eslint 'src/elements.tsx' --fix"
  },
  "peerDependencies": {
    "@emotion/react": "11.x",
    "react": "17.x",
    "react-dom": "17.x"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.11.1"
  }
}
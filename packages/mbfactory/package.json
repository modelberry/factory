{
  "name": "@modelberry/mbfactory",
  "version": "6.30.16",
  "description": "Push Typescript models to a content platform",
  "keywords": [
    "any",
    "cli",
    "component",
    "contentful",
    "gatsbyjs",
    "netlify",
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/modelberry/factory#readme",
  "bugs": {
    "url": "https://github.com/modelberry/factory/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/modelberry/factory.git"
  },
  "license": "MIT",
  "author": "Jacco Meijer (https://www.jaccomeijer.nl)",
  "contributors": [
    {
      "name": "Jacco Meijer",
      "email": "jacco@jaccomeijer.nl",
      "url": "https://www.jaccomeijer.nl"
    }
  ],
  "exports": {
    "./plain": {
      "import": "./build/plain.mjs",
      "require": "./build/plain.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "build/*"
      ]
    }
  },
  "bin": {
    "mbfactory": "build/cli.cjs"
  },
  "files": [
    "CHANGELOG.md",
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:compile": "rollup --config",
    "slow:lint": "eslint --cache 'src/**/*.{ts,tsx}'",
    "build:test": "jest --silent --passWithNoTests",
    "build:types": "tsc -p tsconfig.json --declaration --emitDeclarationOnly --declarationDir build",
    "build:executable": "chmod +x ./build/cli.cjs",
    "clean": "rimraf build/*",
    "dev": "rollup --config --watch",
    "pull-content": "./build/cli.cjs pull content @modelberry/plugin-contentful/plain test",
    "pull-models": "./build/cli.cjs pull models @modelberry/plugin-contentful/plain test",
    "push-content": "./build/cli.cjs push content test/main-content.ts",
    "push-models": "./build/cli.cjs push models test/main.ts",
    "diff-content": "./build/cli.cjs diff content test/main-content.ts",
    "diff-models": "./build/cli.cjs diff models test/main.ts"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.11.1"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "contentful-management": "10.8.0",
    "inquirer": "8.2.3",
    "prettier": "2.7.1",
    "typescript": "4.7.4",
    "yargs": "17.5.1"
  }
}
